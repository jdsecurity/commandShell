server
{
    listen 80;
    server_name web.ganwan.com;
#rewrite ^/(.*)$ http://web.ty366.com/$1 permanent;
    root /var/htmlwww/gwmain/web;
#rewrite "^/(.*)$" http://www.ganwan.com/ permanent;
    location  /shanke/
    {
	rewrite "^/(.*)$" http://web.ganwan.com/sk/ permanent;
    }

    include common.conf;
    access_log /var/slog/nginx/logs/web.ganwan.com_access.log main;
}

